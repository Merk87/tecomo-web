<!DOCTYPE html>
<html>
{% block header %}
    <head>
        <meta name="author" content="Merkury" />
        <meta name="description" content="Tecomo Studio" />
        <meta name="robots" content="index, follow"/>
        <meta name="Language" content="ES" />
        <meta name="country" content="ES" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />

        <!-- Fonts -->
        <link href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css" rel="stylesheet" type="text/css">
        <link href='https://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
        <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>

        {% block stylesheet %}
            <link href="https://netdna.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet" type="text/css">
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/landingbundle/css/fonts.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/landingbundle/css/tecomo.css') }}">
        {% endblock %}

        {% block customstyle %}
        {% endblock %}

        {% block scripts %}
            <script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js"></script>
            <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.2/jquery-ui.min.js"></script>
        {% endblock %}

        {% block customscripts %}
        {% endblock %}

        <link rel="shortcut icon" href="{{ asset('bundles/landingbundle/img/favicon.ico') }}" type="image/x-icon">
        <link rel="icon" href="{{ asset('bundles/landingbundle/img/favicon.ico') }}" type="image/x-icon">
        <title>{% block title %}Tecomo Studios{% endblock %}</title>
    </head>
{% endblock %}
<body id="page-top" data-spy="scroll" data-target=".navbar-custom">
{% block navbar %}
    <nav class="navbar navbar-custom navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header page-scroll">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-main-collapse">
                    <i class="fa fa-bars"></i>
                </button>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse navbar-right navbar-main-collapse">
                <ul class="nav navbar-nav">
                    <!-- Hidden li included to remove active class from about link when scrolled up past about section -->
                    <li class="hidden">
                        <a href="#page-top"></a>
                    </li>
                    <li class="page-scroll">
                        <a href="#why">¿Qué te ofrecemos?</a>
                    </li>
                    <li class="page-scroll">
                        <a href="#us">¿Por qué nosotros?</a>
                    </li>
                    <li class="page-scroll">
                        <a href="#technologies">¿Con que trabajamos?</a>
                    </li>
                    <li class="page-scroll">
                        <a href="#contact">¿Hablamos?</a>
                    </li>
                    <li class="page-scroll">
                        <a href="#copyright">:)</a>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>
{% endblock %}
{% block content %}

{% endblock %}
</body>
{% block footer %}
    {% block js %}
        <script src="{{ asset('bundles/landingbundle/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('bundles/landingbundle/js/tecomo.js') }}"></script>
    {% endblock %}
    {% block customjs %}

    {% endblock %}
    {% block analytics %}
   <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-43527833-1', 'tecomostudios.com');
      ga('send', 'pageview');

    </script>
    {% endblock %}
{% endblock %}
</html>
